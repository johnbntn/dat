.decl callgraph(src:symbol, dst:symbol)
.input callgraph

.decl path(src:symbol, dst:symbol)
path(src, dst) :- callgraph(src, dst).
path(src, dst) :- path(src, x), callgraph(x, dst).

.decl check_path(a:symbol)
.output check_path
check_path("Calls") :- path("@src", "@dst").
check_path("Calls") :- path(p, "@src"), path(q, "@dst"), path(p, q).